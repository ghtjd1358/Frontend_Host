{
  "name": "portfolio-container",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=4096 webpack --config webpack.prod.js",
    "analyze": "cross-env NODE_OPTIONS=--max_old_space_size=4096 webpack --config webpack.prod.js --profile --json > stats.json",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@lottiefiles/dotlottie-react": "^0.17.12",
    "@reduxjs/toolkit": "^2.11.2",
    "@sonhoseong/mfa-lib": "^1.3.8",
    "@supabase/supabase-js": "^2.93.3",
    "axios": "^1.13.2",
    "firebase": "^12.8.0",
    "jsonwebtoken": "^9.0.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vercel/node": "^5.5.28",
    "babel-loader": "^10.0.0",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.5",
    "prettier": "^3.2.0",
    "style-loader": "^4.0.0",
    "typescript": "^5.9.3",
    "webpack": "^5.103.0",
    "webpack-bundle-analyzer": "^4.10.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  }
}
